  "scripts": {
    "build": "webpack -d --optimize-dedupe --config webpack.config.js && npm run test",
    "watch": "webpack --progress --watch --devtool eval --config $3",
    "test": "NODE_ENV=test mocha --require test/setup --recursive test/**/*.js --compilers js:babel-register,scss:test/compilers",
    "easytest": "mocha-webpack --recursive --exit false --require source-map-support/register.js --require ./config/test-setup.js --webpack-config webpack.config.test.js apps/easy-insights/test || true",
    "easytest-jenkins": "JUNIT_REPORT_PATH=easytest-report.xml mocha-webpack --recursive --exit false --require source-map-support/register.js --require ./config/test-setup.js --webpack-config webpack.config.test.js apps/easy-insights/test --reporter mocha-jenkins-reporter || true",
    "easytest:watch": "npm run easytest -- --watch",
    "easydev": "webpack-dev-server --inline --port 8000 --progress --colors --hot --content-base dist --config apps/easy-insights/webpack.config.dev.js",
    "errordev": "webpack-dev-server --inline --port 8000 --progress --colors --hot --content-base dist --config apps/error-app/webpack.config.dev.js"
  },
